<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cameralib">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <application
        android:allowBackup="true"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:resizeableActivity="true"
        android:supportsRtl="true">
        <activity
            android:name=".ActivityCamrea"
            android:screenOrientation="portrait" />
        <activity android:name=".camera_result.ActivityTakePhotoView"
            android:screenOrientation="portrait"/>
        <activity android:name=".selectimg.ActivityLocalImageList" android:screenOrientation="portrait"/>
        <activity android:name=".selectimg.ActivityLocalImagePager" android:screenOrientation="portrait"/>
    </application>
</manifest>