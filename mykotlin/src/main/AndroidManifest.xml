<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mykotlin">



<!--    android:icon="@mipmap/ic_launcher"-->
<!--    android:roundIcon="@mipmap/ic_launcher_round"-->
    <!--蓝牙权限-->
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <!--让应用启动设备发现或操纵蓝牙设置-->
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <!-- LE Beacons位置相关权限-->
    <!-- 如果设配Android9及更低版本，可以申请 ACCESS_COARSE_LOCATION -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--ble模块 设置为true表示只有支持ble的手机才能安装-->
    <uses-feature
        android:name="android.hardware.bluetooth_le"
        android:required="true" />
    <application
        android:name=".KtApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_config"
        android:supportsRtl="true"
        android:resizeableActivity="true"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name=".activity_demo.ActivityCoroutines">

        </activity>
        <activity android:name=".activity.activity_bluetouch.BluetouchActivity"
            android:theme="@style/AppTheme"
            android:label="title_activity_bubble"
            android:allowEmbedded="true"
            android:resizeableActivity="true"
            >

        </activity>
   <activity android:name=".activity.activity_bluetouch.ActivityBluetooth"
            android:theme="@style/AppTheme"
            android:label="title_activity_bubble"
            android:allowEmbedded="true"
            android:resizeableActivity="true"
            >

        </activity>

        <activity android:name=".activity.activity_music.ActivityMusic"
            android:screenOrientation="landscape"
            />

        <activity android:name=".activity.activity_notification.ActivityNotification">

<!--            <intent-filter>-->
<!--                <action android:name="android.intent.action.MAIN" />-->
<!--                <category android:name="android.intent.category.LAUNCHER" />-->
<!--            </intent-filter>-->
        </activity>

    </application>
</manifest>